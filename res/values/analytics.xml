<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools" tools:ignore="TypographyDashes">
    
    <!--
    	By default, the reported app name and version come from the package.  
     -->

    <!-- Replace placeholder ID with your tracking ID -->
    <string name="ga_trackingId" tools:ignore="UnusedResources">UA-33660954-1</string>

    <!-- Enable Activity tracking -->
    <bool name="ga_autoActivityTracking" tools:ignore="UnusedResources">true</bool>
    
    <!-- 
    	Set how often auto dispatch gets fired. Should be set to 30 minutes
    	before app is released. 
    -->
    <integer name="ga_dispatchPeriod" tools:ignore="UnusedResources">1</integer>
    
    <!-- Set how often auto dispatch gets fired -->
    <integer name="ga_sessionTimeout" tools:ignore="UnusedResources">60</integer>
    
    <!-- Privacy settings that strips the last octet of the IP before storage in service -->
    <bool name="ga_anonymizeIp" tools:ignore="UnusedResources">false</bool>
    
    <!-- Info on what is being tracked is output to logcat -->
    <bool name="ga_debug">true</bool>

    <!-- The screen names that will appear in the reporting -->
    <string name="com.aircandi.CandiRadar">Radar</string>
    <string name="com.aircandi.CandiUser">User</string>
    <string name="com.aircandi.SignInForm">Signin</string>
    <string name="com.aircandi.SignUpForm">Signup</string>
    <string name="com.aircandi.ProfileForm">Profile</string>
    <string name="com.aircandi.PasswordForm">Password</string>
    <string name="com.aircandi.Preferences">Preferences</string>
    <string name="com.aircandi.CandiList">CandiList</string>
    <string name="com.aircandi.CandiForm">CandiForm</string>
    <string name="com.aircandi.CandiPicker">CandiPicker</string>
    <string name="com.aircandi.TemplatePicker">TemplatePicker</string>
    <string name="com.aircandi.PictureSearch">PictureSearch</string>
    <string name="com.aircandi.PictureBrowse">PictureBrowse</string>
    <string name="com.aircandi.EntityForm">Entity</string>
    <string name="com.aircandi.CommentForm">Comment</string>
    <string name="com.aircandi.CommentList">CommentList</string>

</resources>