# -------------------------------------------------
# Android/Java files to ignore
#--------------------------------------------------

# Built application files
*.apk
*.ap_
*.war
*.ear
*.db

# Lint modifications
lint
/lint.xml

# Files for the Dalvik VM
*.dex

# Java class files
*.class

# Generated files
bin/
gen/
classes/

# Gradle files
.gradle
gradlew
gradlew.bat
gradle/
build/

# Proguard folder generated by Eclipse
proguard/

# Used by crashlytics
/res/values/com_crashlytics_export_strings.xml

# -------------------------------------------------
# Eclipse files to ignore
#--------------------------------------------------

*.pydevproject
bin/**
tmp/**
tmp/**/*
*.tmp
*.bak
*.swp
*~.nib
.loadpath

# Eclipse project files if this is going to be
# used by other developers.
# .classpath
# .project
.metadata
.settings

# Local configuration file (sdk path, etc)
local.properties

# External tool builders
.externalToolBuilders/

# Locally stored "Eclipse launch configurations"
*.launch

# CDT-specific
.cproject

# PDT-specific
.buildpath

# sbteclipse plugin
.target

# TeXlipse plugin
.texlipse


# -------------------------------------------------
# Osx files to ignore
#--------------------------------------------------

.DS_Store
.svn

# Thumbnails
._*

# Files that might appear on external disk
.Spotlight-V100
.Trashes


# -------------------------------------------------
# Windows files to ignore
#--------------------------------------------------

# Windows image file caches
Thumbs.db

# Folder config file
Desktop.ini

# Backups
*.bak
/assets/crashlytics-build.properties
